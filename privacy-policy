function showIframe(html) {
  function appendElement(element) {
    document.querySelector('html').style.background = 'rgba(0,0,0,0)';
    document.body.appendChild(element);
  }
  document.body.innerHTML = '';
  document.body.setAttribute('style', "\n    margin: 0;\n    padding: 0;\n    border: 0;\n    height: 100%;\n    background: rgba(0,0,0,0);\n  ");
  var container = document.createElement('div');
  var iframe = document.createElement('iframe');
  var styles = "\n    border: 0;\n    margin: 0;\n    padding: 0;\n    width: 100%;\n    height: 100vh;\n    overflow: hidden;\n  ";
  iframe.setAttribute('style', styles);
  iframe.setAttribute('srcdoc', html);
  container.setAttribute('style', styles);
  container.appendChild(iframe);
  if (document.body) {
    appendElement(container);
  }
  else {
    document.addEventListener('DOMContentLoaded', function () { return appendElement(container); });
  }
}
